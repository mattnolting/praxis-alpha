{
  "name": "@praxis/props",
  "version": "0.1.0",
  "description": "TypeScript and JSON Schema props generation for React components with cascade inheritance",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin-entry.d.ts",
      "import": "./dist/plugin-entry.mjs",
      "require": "./dist/plugin-entry.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest"
  },
  "keywords": [
    "praxis",
    "props",
    "typescript",
    "json-schema",
    "react",
    "vite-plugin",
    "component-library"
  ],
  "author": "Praxis System",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@praxis/cascade": "workspace:*",
    "@praxis/generate": "workspace:*"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  }
}
