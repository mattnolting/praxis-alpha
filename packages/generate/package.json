{
  "name": "@praxis/generate",
  "version": "0.1.0",
  "description": "Generic code generation framework and caching system for Praxis",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest"
  },
  "keywords": [
    "praxis",
    "generation",
    "cache",
    "semantic-engine",
    "performance",
    "framework"
  ],
  "author": "Praxis System",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@praxis/cascade": "workspace:*"
  }
}
